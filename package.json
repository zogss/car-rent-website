    "bcrypt": "^5.0.1",
    "bootstrap": "5.2.0-beta1",
    "bootstrap-icons": "^1.9.1",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-handlebars": "^6.0.6",
    "express-session": "^1.17.3",
    "handlebars": "^4.7.7",
    "laravel-mix": "^6.0.49",
    "method-override": "^3.0.0",
    "mongoose": "^6.4.4",
    "morgan": "^1.9.1",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^2.9.0",
    "passport": "^0.6.0",
